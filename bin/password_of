#!/usr/bin/env ruby
# -*- coding: utf-8 -*-
# vim:ft=ruby:enc=utf-8

base_path = ( File.expand_path(File.dirname(__FILE__)+'/..') )
$LOAD_PATH << base_path unless $LOAD_PATH.include?(base_path)

require 'lib/to_pass'

ToPass::Cli.new({
  :algorithm => 'basic_de',
  :pipe_out  => false,
  :pipe_in   => false
}).output
