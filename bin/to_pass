#!/usr/bin/env ruby
# -*- coding: utf-8 -*-
# vim:ft=ruby:fileencoding=utf-8

base_path = File.expand_path('../../', __FILE__)
$LOAD_PATH << base_path unless $LOAD_PATH.include?(base_path)
$LOAD_PATH << "#{base_path}/lib" unless $LOAD_PATH.include?("#{base_path}/lib")

require 'to_pass'

ToPass::Cli.new({
  :algorithm => 'basic_de',
  :pipe_out  => false,
  :pipe_in   => false
}).output
